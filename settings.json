{
  "explorer.confirmDelete": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "twig": "html"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "json.schemas": [],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[html.twig]": {
    "editor.defaultFormatter": null
  },

  "liveSassCompile.settings.generateMap": false,
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css"
    }
  ],
  "gitlens.hovers.enabled": false,
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.blame.avatars": false,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.blame.compact": false,
  "gitlens.changes.locations": [],
  "gitlens.heatmap.locations": [],
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "settingsSync.ignoredExtensions": [],
  "workbench.colorCustomizations": {
    // Gutter indicators (left)
    "editorGutter.modifiedBackground": "#0000",
    "editorGutter.addedBackground": "#0000",
    "editorGutter.deletedBackground": "#0000"
    // Scrollbar indicators (right)
    /*     "editorOverviewRuler.addedForeground": "#0000",
      "editorOverviewRuler.modifiedForeground": "#0000",
      "editorOverviewRuler.deletedForeground": "#0000" */
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

  // Add these settings to your VS Code's existing settings.json file (2020)
  "php.suggest.basic": false,
  "[php]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  "php-cs-fixer.executablePath": "C:\\Users\\tigre\\AppData\\Roaming\\Composer\\vendor\\bin\\php-cs-fixer.bat",
  "php-cs-fixer.executablePathWindows": "C:\\Users\\tigre\\AppData\\Roaming\\Composer\\vendor\\bin\\php-cs-fixer.bat",
  "php-cs-fixer.onsave": true,
  "php-cs-fixer.rules": "@PSR2",
  "php-cs-fixer.config": "C:\\Users\\tigre\\AppData\\Roaming\\Composer\\vendor\\bin\\config.php_cs",
  "php-cs-fixer.allowRisky": false, //it is safe not to allow risky linting
  "php-cs-fixer.pathMode": "override",
  "php-cs-fixer.exclude": [],
  "php-cs-fixer.autoFixByBracket": true,
  "php-cs-fixer.autoFixBySemicolon": false,
  "php-cs-fixer.formatHtml": true,
  "php-cs-fixer.documentFormattingProvider": true
}
